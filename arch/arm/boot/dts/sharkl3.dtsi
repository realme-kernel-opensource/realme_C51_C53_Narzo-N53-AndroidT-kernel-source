					reg = <0x9 0x4>;
					bits = <4 4>;
				};

				wcn_alpha: wcn-alpha@14 {
					reg = <0x14 0x4>;
				};

				wcn_beta: wcn-beta@18 {
					reg = <0x18 0x4>;
				};

				wcn_gamma: wcn-gamma@1c {
					reg = <0x1c 0x4>;
				};

				wcn_delta: wcn-delta@20 {
					reg = <0x20 0x4>;
				};

				gthm_delta: gthm-delta@21{
					reg = <0x21 0x4>;
